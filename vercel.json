{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/js/(.*)",
            "dest": "/js/$1"
        },
        {
            "src": "/css/(.*)",
            "dest": "/css/$1"
        },
        {
            "src": "/img_cache/(.*)",
            "dest": "/img_cache/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/dashboard",
            "dest": "/main.py"
        },
        {
            "src": "/(exhibitors|people|speakers|sessions|sponsors|events|settings)",
            "dest": "/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "/main.py"
        }
    ],
    "crons": [
        {
            "path": "/api/cron/sync",
            "schedule": "0 * * * *"
        }
    ]
}