{
    "version": 2,
    "functions": {
        "main.py": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/main.py"
        },
        {
            "source": "/(exhibitors|people|speakers|sessions|sponsors|events|settings|dashboard)",
            "destination": "/main.py"
        }
    ],
    "crons": [
        {
            "path": "/api/cron/sync",
            "schedule": "0 0 * * *"
        }
    ]
}