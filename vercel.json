{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/main.py"
        },
        {
            "source": "/js/(.*)",
            "destination": "/js/$1"
        },
        {
            "source": "/css/(.*)",
            "destination": "/css/$1"
        },
        {
            "source": "/img_cache/(.*)",
            "destination": "/img_cache/$1"
        },
        {
            "source": "/(exhibitors|people|speakers|sessions|sponsors|events|settings|dashboard)",
            "destination": "/main.py"
        }
    ],
    "crons": [
        {
            "path": "/api/cron/sync",
            "schedule": "0 0 * * *"
        }
    ]
}